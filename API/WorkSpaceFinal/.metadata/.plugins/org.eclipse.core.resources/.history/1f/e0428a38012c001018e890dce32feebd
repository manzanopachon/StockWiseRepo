package com.dam.restaurante.dto;

public class CategoriaDTO {
	private String nombre;

	
	public CategoriaDTO(String nombre) {
		super();
		this.nombre = nombre;
	}

	public CategoriaDTO() {
		super();
	}

	public String getNombre() {
		return nombre;
	}

	public void setNombre(String nombre) {
		this.nombre = nombre;
	}
		
}
